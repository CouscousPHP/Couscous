{
    "name": "couscous/couscous",
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Couscous\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Couscous\\Tests\\": "tests/"
        }
    },
    "bin": [
        "bin/couscous"
    ],
    "require": {
        "php": ">=5.4.0",
        "symfony/console": "~2.4",
        "symfony/filesystem": "~2.4",
        "symfony/finder": "~2.4",
        "symfony/process": "~2.4",
        "symfony/yaml": "~2.4",
        "twig/twig": "~1.0",
        "erusev/parsedown": "~1.1",
        "erusev/parsedown-extra": "~0.2.0",
        "phine/phar": "~1.0",
        "mnapoli/front-yaml": "~1.0",
        "mnapoli/php-di": "~4.4",
        "beelab/bowerphp": "1.0.x-dev",
        "league/commonmark": "dev-master@dev"
    },
    "require-dev": {
        "phpunit/phpunit": "~4.3"
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
